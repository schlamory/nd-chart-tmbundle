	<string>nop</string>
	<key>command</key>
	<string>#!/usr/bin/env ruby
require ENV["ND_CHART_BUNDLE"] + "/src/nd"

visit = Nd::Visit.initialize_from_dir ENV["TM_DIRECTORY"]

note_type = case ENV["TM_FILENAME"]
when "progress_note.md" then "progress"
when "encounter_note.md" then "encounter"
end

visit.preview_note(note_type)

